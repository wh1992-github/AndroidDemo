<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.event">

    <!-- SD卡 -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity.KeySoftActivity" />
        <activity android:name=".activity.KeyHardActivity" />
        <activity android:name=".activity.VolumeSetActivity" />
        <activity android:name=".activity.GestureDetectorActivity" />
        <activity android:name=".activity.ViewFlipperActivity" />
        <activity android:name=".activity.BannerFlipperActivity" />
        <activity android:name=".activity.EventDispatchActivity" />
        <activity android:name=".activity.EventInterceptActivity" />
        <activity android:name=".activity.TouchSingleActivity" />
        <activity android:name=".activity.TouchMultipleActivity" />
        <activity android:name=".activity.SignatureActivity" />
        <activity android:name=".activity.CustomScrollActivity" />
        <activity android:name=".activity.DisallowScrollActivity" />
        <activity android:name=".activity.DrawerLayoutActivity" />
        <activity
            android:name=".activity.PullRefreshActivity"
            android:theme="@style/AppCompatTheme" />
        <activity android:name=".activity.ImageChangeActivity" />
        <activity android:name=".activity.ImageCutActivity" />
        <activity android:name=".activity.MeituActivity" />
        <activity android:name=".activity.GlLineActivity" />
        <activity android:name=".activity.GlGlobeActivity" />
        <activity
            android:name=".activity.GlPanoramaActivity"
            android:theme="@style/AppCompatTheme" />

    </application>

</manifest>